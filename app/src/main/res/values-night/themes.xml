<resources xmlns:tools="http://schemas.android.com/tools">
	<!-- Base application theme. -->
	<style name="Theme.Entry" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
		<!-- Primary brand color. -->
		<item name="colorPrimary">#A8BED6</item><!--@color/blue_900-->
		<item name="colorPrimaryVariant">#A8BED6</item><!--@color/blue_900-->
		<item name="colorOnPrimary">@color/black</item>
		<!-- Secondary brand color. -->
		<item name="colorSecondary">#FAE9BB</item><!--@color/cyan_200-->
		<item name="colorSecondaryVariant">#FAE9BB</item><!--@color/cyan_700-->
		<item name="colorOnSecondary">@color/black</item>
		<!-- Status bar color. -->
		<item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
		<!-- Customize your theme here. -->
<!--		<item name="android:windowTranslucentStatus">true</item>-->
		<item name="android:windowAnimationStyle">@style/AnimationActivity</item>
		<!-- 失败和成功颜色 -->
		<item name="colorError">@color/red_800</item>
		<item name="colorSuccess">@color/green_800</item>
		<item name="sendBarBgColor">@color/blue_800</item>
		<item name="textColorSecondary">@color/grey_200</item>
	</style>
</resources>